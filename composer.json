{
	"name": "jeffstieler/jeffstieler.com",
	"description": "Source of jeffstieler.com",
	"license": "proprietary",
	"authors": [
		{
			"name": "Jeff Stieler",
			"email": "jeff.m.stieler@gmail.com"
		}
	],
	"config": {
		"preferred-install": "dist",
		"generate-salts": true
	},
	"autoload": {
		"psr-0": {"Roots\\Bedrock\\Installer": "scripts"}
	},
	"scripts": {
		"post-root-package-install": ["Roots\\Bedrock\\Installer::addSalts"]
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://wpackagist.org"
		}
	],
	"require": {
		"php": ">=5.4",
		"johnpbloch/wordpress": "4.1",
		"composer/installers": "v1.0.12",
		"vlucas/phpdotenv": "1.0.9",
		"wpackagist-theme/twentyfifteen": "~1.0"
	},
	"extra": {
		"installer-paths": {
			"web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
			"web/app/plugins/{$name}/": ["type:wordpress-plugin"],
			"web/app/themes/{$name}/": ["type:wordpress-theme"]
		},
		"wordpress-install-dir": "web/wp"
	}
}